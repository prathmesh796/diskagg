{
    "manifest_version": 3,
    "name": "Kaggle â†’ Discord Status",
    "version": "1.0.0",
    "description": "Share your Kaggle achievements automatically to Discord.",
    "permissions": [
        "storage",
        "alarms",
        "notifications"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "128": "icons/icon128.png"
        }
    },
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "128": "icons/icon128.png"
    }
}